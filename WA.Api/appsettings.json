{
  "ConnectionStrings": {
    "DefaultSqlConnection": "Data Source=App_Data/crypto-data.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WhatsAppFbConfiguration": {
    "VerificationToken": "[the token you set in fb app configuration page]",
    "PhoneNumberId": "[your whatsapp phone number id",
    "FbApiVersionId": "v13.0",
    "FbAccessToken": "[your fb token]"
  },
  "ApiProviders": {
    "LiveCoinWatch": {
      "Name": "LiveCoinWatch",
      "ApiMaxLimitCall": 10000,
      "HeaderKeyName": "x-api-key",
      "HeaderKeyValue": "[the key obtained from livecoinwatch api]",
      "BaseEndPoint": "https://api.livecoinwatch.com",
      "SingleDataEndPoint": "/coins/single",
      "SingleDataHistoryEndPoint": "/coins/single/history",
      "ApiDailyUsageEndPoint": "/credits"
    }
  }
}

